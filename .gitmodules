[submodule "zsh/.zsh-plugins/zsh-diff-so-fancy"]
	path = zsh/.zsh-plugins/zsh-diff-so-fancy
	url = https://github.com/zdharma/zsh-diff-so-fancy
[submodule "zsh/.zsh-plugins/anyframe"]
	path = zsh/.zsh-plugins/anyframe
	url = https://github.com/mollifier/anyframe
[submodule "zsh/.zsh-plugins/zsh-async"]
	path = zsh/.zsh-plugins/zsh-async
	url = https://github.com/mafredri/zsh-async
[submodule "zsh/.zsh-plugins/powerlevel10k"]
	path = zsh/.zsh-plugins/powerlevel10k
	url = https://github.com/romkatv/powerlevel10k
[submodule "zsh/.zsh-plugins/zsh-256color"]
	path = zsh/.zsh-plugins/zsh-256color
	url = https://github.com/chrissicool/zsh-256color.git
[submodule "zsh/.zsh-plugins/ohmyzsh"]
	path = zsh/.zsh-plugins/ohmyzsh
	url = https://github.com/ohmyzsh/ohmyzsh
[submodule "dotbot"]
	path = dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
